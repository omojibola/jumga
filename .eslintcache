dleBlur}\r\n                    value={values.productName}\r\n                  />\r\n                  <ErrorWrapper>\r\n                    <Error name=\"productName\" />\r\n                  </ErrorWrapper>\r\n\r\n                  <Label>Price</Label>\r\n                  <FormInput\r\n                    type=\"number\"\r\n                    name=\"price\"\r\n                    placeholder=\"Price in USD\"\r\n                    onChange={handleChange}\r\n                    onBlur={handleBlur}\r\n                    value={values.price}\r\n                  />\r\n                  <ErrorWrapper>\r\n                    <Error name=\"price\" />\r\n                  </ErrorWrapper>\r\n                  <Label>Description</Label>\r\n                  <TextArea\r\n                    cols={40}\r\n                    name=\"description\"\r\n                    placeholder=\"Description\"\r\n                    onChange={handleChange}\r\n                    onBlur={handleBlur}\r\n                    value={values.description}\r\n                  />\r\n                  <ErrorWrapper>\r\n                    <Error name=\"description\" />\r\n                  </ErrorWrapper>\r\n                  <Label>Product Image</Label>\r\n                  <FormInput\r\n                    type=\"file\"\r\n                    name=\"productImage\"\r\n                    id=\"file\"\r\n                    onChange={(event) => {\r\n                      setFieldValue(\r\n                        'productImage',\r\n                        event.currentTarget.files[0]\r\n                      );\r\n                    }}\r\n                    onBlur={handleBlur}\r\n                  />\r\n                  <SubmitButton type=\"submit\" disabled={isSubmitting}>\r\n                    {loading ? 'Submitting...' : 'Submit'}\r\n                  </SubmitButton>\r\n                </StyledForm>\r\n              )}\r\n            </FormContainer>\r\n          </div>\r\n        </Fade>\r\