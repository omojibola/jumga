};\n\n  useEffect(() => {\n    fetchProfileDetails();\n  }, []);\n\n  //log out user\n  const logOut = async () => {\n    await startSignOut();\n    history.push('/');\n    window.location.reload();\n  };\n\n  const renderPage = () => {\n    if (loading) {\n      return <h1>Loading...</h1>;\n    } else\n      return (\n        <Fragment>\n          <h2>Account</h2>\n\n          {profileDetail.status && <h3>Status: {profileDetail.status}</h3>}\n          {dispatchRider && <h3>Status: {dispatchRider.name}</h3>}\n\n          <Button color=\"danger\" onClick={() => logOut()}>\n            Sign Out\n          </Button>\n          <div style={{ fontSize: 20 }}>\n            <Link to=\"/\">Back Home</Link>\n          </div>\n        </Fragment>\n      );\n  };\n  return (\n    <div style={{ textAlign: 'center', padding: 200 }}>{renderPage()}</div>\n  );\n};\n\nconst mapStateToProps = ({ profile }) => ({\n  profileDetail: profile.data,\n  dispatchRider: profile.data.dispatchRider,\n  loading: profile.loading,\n});\n\nconst mapDispatchToProps = {\n  fetchProfile: actions.fetchProfile,\n  startSignOut: SignOut,\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Account);\n","/Users/joshuaolorunnipa/Downloads/React Guide/jumga/src/components/Payment/Payment.js",[],"/Users/joshuaolorunnipa/Downloads/React Guide/jumga/src/components/Payment/PaymentConfirmed.js",[],"/Users/joshuaolorunnipa/Downloads/React Guide/jumga/src/components/Payment/PaymentTwo.js",["265"],"import React from \"react\";\nimport {\n    Details,\n    FormContainer, FormInput, FormItem, FormLabel, FormLine, PaymentInfo,\n    ListItem,\n    OrderedList, PaymentForm,\n    PaymentHeading,\n    PayParagraph, FormSelect,\n    ReturnLink, StyledForm\n} from \"./PaymentElements\";\nimport {LinkTag} from \"../Header/HeaderElements\";\nimport ButtonName from \"../Button/ButtonName\";\nimport OrderTotal from \"../OrderTotal\";\nimport {useHistory} from \"react-router-dom\";\nimport {useSelector} from \"re